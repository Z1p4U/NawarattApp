{
  "expo": {
    "name": "Nawaratt App",
    "slug": "NawarattApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/placeholder.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "video": "./assets/videos/splash.mp4",
      "image": "./assets/images/placeholder.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/videos/*",
      "assets/sounds/*"
    ],
    "notification": {
      "icon": "./assets/images/placeholder.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "Nawaratt App"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.z1p4u.NawarattApp",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/placeholder.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.z1p4u.NawarattApp",
      "permissions": ["INTERNET", "INTERNET"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "android": {
            "useNextNotificationsApi": true,
            "extraResources": [
              {
                "source": "./assets/sounds/nawaratt.wav",
                "target": "app/src/main/res/raw/nawaratt.wav"
              },
              {
                "source": "./assets/sounds/custom.wav",
                "target": "app/src/main/res/raw/custom.wav"
              }
            ]
          },
          "ios": {
            "deploymentTarget": "15.1",
            "assetPackPatterns": [
              "assets/sounds/nawaratt.wav",
              "assets/sounds/custom.wav"
            ]
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/placeholder.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ab21b8ae-9f5d-4dd0-b775-d803f37953d5"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ab21b8ae-9f5d-4dd0-b775-d803f37953d5"
    }
  }
}
