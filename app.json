{
  "expo": {
    "name": "Nawaratt App",
    "slug": "NawarattApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/placeholder.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": ["assets/images/*"],
    "notification": {
      "icon": "./assets/images/placeholder.png",
      "color": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.z1p4u.NawarattApp",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/placeholder.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.z1p4u.NawarattApp",
      "permissions": ["INTERNET"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "android": { "useNextNotificationsApi": true },
          "ios": { "deploymentTarget": "15.1" }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/placeholder.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ab21b8ae-9f5d-4dd0-b775-d803f37953d5"
      }
    }
  }
}
